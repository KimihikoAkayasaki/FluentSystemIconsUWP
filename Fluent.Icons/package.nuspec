<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>Fluent.Icons.Compact</id>
    <version>1.1.125.0</version>
    <title>Fluent.Icons.Compact</title>
    <authors>Joshua Askharoun</authors>
    <owners>Joshua Askharoun</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">LGPL-3.0-only</license>
    <projectUrl>https://github.com/yoshiask/FluentSystemIconsUWP/</projectUrl>
    <repository type="git" url="https://github.com/yoshiask/FluentSystemIconsUWP" />
    <icon>Logo.png</icon>
    <description>An unofficial UWP library that provides easy access to Microsoft's Fluent System Icons. Requires UAP &gt;= 10.0.15063. This version of Fluent.Icons uses an icon font, which reduces the library size at the expense of explicit scaling (e.g. 16, 24, 48) options.</description>
    <releaseNotes>Updated library to sync with FSI 1.1.125 and fixed font embedding</releaseNotes>
    <copyright>Copyright 2020</copyright>
    <tags>UWP Fluent Design UI Microsoft Fabric Icons</tags>
    <dependencies>
      <group targetFramework="uap10.0.15063">
        <dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="6.2.10" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!-- Dll -->
    <file src="bin\Release\Fluent.Icons.dll" target="lib\uap10.0.15063" />
    <!-- Resources -->
    <file src="bin\Release\Fluent.Icons.pdb" target="lib\uap10.0.15063" />
    <file src="bin\Release\Fluent.Icons.pri" target="lib\uap10.0.15063" />
    <file src="bin\Release\Assets\FluentUI-System-Icons.ttf" target="lib\uap10.0.15063" />
    <!-- IntelliSense -->
    <file src="bin\Release\Fluent.Icons.xml" target="lib\uap10.0.15063" />
    <!-- XAML control -->
    <file src="bin\Release\Fluent.Icons\**\*.*" target="lib\uap10.0.15063\Fluent.Icons" />
    <!-- Icon -->
    <file src="Logo.png" />
  </files>
</package>