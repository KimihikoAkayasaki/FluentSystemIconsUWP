<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>Fluent.Icons</id>
        <version>1.1.59.0</version>
        <title>Fluent.Icons</title>
        <authors>Joshua Askharoun</authors>
        <owners>Joshua Askharoun</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <license type="expression">MIT</license>
        <projectUrl>https://github.com/yoshiask/FluentSystemIconsUWP/</projectUrl>
				<repository type="git" url="https://github.com/yoshiask/FluentSystemIconsUWP.git" />
				<icon>Logo.png</icon>
        <description>An unofficial UWP library that provides easy access to Microsoft's Fluent System Icons. Requires UAP >= 10.0.15063.</description>
        <releaseNotes>Adds all icons, not just icons that have a 24px size.</releaseNotes>
        <copyright>Copyright 2020</copyright>
        <tags>UWP Fluent Design UI Microsoft Fabric Icons</tags>

			<dependencies>
				<group targetFramework="uap10.0.15063">
					<dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="6.2.10" exclude="Build,Analyzers" />
				</group>
			</dependencies>
    </metadata>

    <files>
			<!-- Dll -->
			<file src="bin\Release\Fluent.Icons.dll" target="lib\uap10.0.15063" />

			<!-- Resources -->
			<file src="bin\Release\Fluent.Icons.pdb" target="lib\uap10.0.15063" />
			<file src="bin\Release\Fluent.Icons.pri" target="lib\uap10.0.15063" />

			<!-- IntelliSense -->
			<file src="bin\Release\Fluent.Icons.xml" target="lib\uap10.0.15063" />

			<!-- XAML control -->
			<file src="bin\Release\Fluent.Icons\**\*.*" target="lib\uap10.0.15063\Fluent.Icons" />

			<!-- Icon -->
			<file src="Logo.png"/>
		</files>
</package>