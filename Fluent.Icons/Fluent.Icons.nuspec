<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>Fluent.Icons</id>
        <version>0.1.0.0-alpha</version>
        <title>Fluent.Icons</title>
        <authors>Joshua Askharoun</authors>
        <owners>Joshua Askharoun</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <license type="expression">MIT</license>
        <projectUrl>https://github.com/yoshiask/FluentSystemIconsUWP/</projectUrl>
        <!--<icon>https://github.com/microsoft/fluentui-system-icons/raw/master/art/readme-asset.png</icon>-->
        <description>An unofficial UWP library that provides easy access to Microsoft's Fluent System Icons. Requires UAP >= 10.0.16299.</description>
        <releaseNotes>Created library, generated icons from commit 52fbfa9</releaseNotes>
        <copyright>Copyright 2020</copyright>
        <tags>UWP Fluent Design UI Microsoft Fabric Icons</tags>

		<dependencies>
			<group targetFramework="uap10.0.16299">
			</group>
		</dependencies>
    </metadata>

    <files>
        <!-- WinMd and IntelliSense files -->
        <file src="bin\Release\Fluent.Icons.dll" target="lib\uap10.0.16299"/>
        <file src="bin\Release\Fluent.Icons.xml" target="lib\uap10.0.16299"/>
        
        <!-- XAML controls -->
        <file src="Themes\Generic.xaml" target="lib\uap10.0.16299\Themes"/>
        
        <!-- WINMDs and resources -->
        <file src="bin\ARM\Release\Fluent.Icons.dll" target="runtimes\win10-arm"/>
        <file src="bin\ARM\Release\Fluent.Icons.pri" target="runtimes\win10-arm"/>
        
        <file src="bin\ARM64\Release\Fluent.Icons.dll" target="runtimes\win10-arm64"/>
        <file src="bin\ARM64\Release\Fluent.Icons.pri" target="runtimes\win10-arm64"/>
        
        <file src="bin\x64\Release\Fluent.Icons.dll" target="runtimes\win10-x64"/>
        <file src="bin\x64\Release\Fluent.Icons.pri" target="runtimes\win10-x64"/>
        
        <file src="bin\x86\Release\Fluent.Icons.dll" target="runtimes\win10-x86"/>
        <file src="bin\x86\Release\Fluent.Icons.pri" target="runtimes\win10-x86"/>
